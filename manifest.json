{
  "name": "Dooray comment file download",
  "description": "dooray only",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://nhnent.dooray.com/project/*"],
      "run_at": "document_end",
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
